@model MVCEmail.Models.EmailFormModel


@{
    ViewBag.Title = "Sent";
}
<h2>Your message has been sent Please Check Your E-mail </h2>
@using (Html.BeginForm())
{

    <div class="form-group">
        @Html.LabelFor(m => m.EntryCode, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.EntryCode, new { @class = "form-control" })
            @Html.ValidationMessageFor(m => m.EntryCode)
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" class="btn btn-default" value="Alp" />
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
